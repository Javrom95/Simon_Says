<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape android:shape="rectangle"  >
            <gradient android:angle="-90" android:startColor="@color/colorbutton12" android:endColor="@color/colorbutton12" />
        </shape>
    </item>



    <item android:state_pressed="true">
        <shape android:shape="rectangle"  >
            <gradient android:angle="-90" android:startColor="@color/colorpressedbutton12" android:endColor="@color/colorpressedbutton12" />
        </shape>
    </item>

</selector>